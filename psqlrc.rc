\set QUIET 1
\set ON_ERROR_ROLLBACK interactive
\set VERBOSITY verbose
\x auto
\set PROMPT1 '%[%033[34;1;27m%]%n%[%033[0m%]@%[%033[31;1;27m%]%m %[%033[0m%][%[%033[33;1;27m%]%/%[%033[0m%]] %x%# %[%033[32;1;27m%]'
\set PROMPT2 '%[%033[0m%](cont) >>> %[%033[32;1;27m%]'
\set HISTFILE ~/.psql_history- :DBNAME
\set HISTCONTROL ignoredups
\set COMP_KEYWORD_CASE upper
\pset null [null]
\pset pager always
\timing
\unset QUIET


